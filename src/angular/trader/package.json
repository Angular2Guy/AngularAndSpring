    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.3",
    "rimraf": "^3.0.2",
    "webpack-bundle-analyzer": "^3.3.2"
